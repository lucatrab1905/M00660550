<?xml version="1.0"?>
<robot name="gazebo1" xmlns:xacro="http://ros.org/wiki/xacro">


  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/</robotNamespace>
      <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
      <legacyModeNS>true</legacyModeNS>
    </plugin>
  </gazebo>



  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>



  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.1" radius="0.25"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.1" radius="0.25"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="20"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

   <gazebo reference="base_link">
     <mu1 value="200.0"/>
     <mu2 value="100.0"/>
     <kp value="10000000.0" />
     <kd value="1.0" />
     <material>Gazebo/Red</material>
   </gazebo>



   <link name="right_back_wheel">
     <visual>
       <origin rpy="1.57075 0 0" xyz="0 0 0"/>
       <geometry>
         <cylinder length="0.05" radius="0.035"/>
       </geometry>
       <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.035"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
   </link>
   <joint name="right_back_wheel_joint" type="continuous">
     <axis rpy="0 0 0" xyz="0 1 0"/>
     <parent link="base_link"/>
     <child link="right_back_wheel"/>
   <origin rpy="0 0 0" xyz="-0.1 -0.15 -0.085"/>
   </joint>

    <gazebo reference="right_back_wheel">
      <mu1 value="200.0"/>
      <mu2 value="100.0"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <material>Gazebo/Grey</material>
    </gazebo>

    <transmission name="right_back_wheel_trans">
      <type>transmission_interface/SimpleTransmission</type>
      <actuator name="right_back_wheel_motor">
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
      <joint name="right_back_wheel_joint">
        <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
      </joint>
    </transmission>

    <link name="left_back_wheel">
      <visual>
        <origin rpy="1.57075 0 0" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.05" radius="0.035"/>
        </geometry>
        <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.035"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
    </link>
    <joint name="left_back_wheel_joint" type="continuous">
      <axis rpy="0 0 0" xyz="0 1 0"/>
      <parent link="base_link"/>
      <child link="left_back_wheel"/>
      <origin rpy="0 0 0" xyz="-0.1 0.15 -0.085"/>
    </joint>


    <gazebo reference="left_back_wheel">
      <mu1 value="200.0"/>
      <mu2 value="100.0"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <material>Gazebo/Grey</material>
    </gazebo>
    <transmission name="left_back_wheel_trans">
      <type>transmission_interface/SimpleTransmission</type>
      <actuator name="left_back_wheel_motor">
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
      <joint name="left_back_wheel_joint">
        <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
      </joint>
    </transmission>


    <link name="front_wheel">
      <visual>
        <origin rpy="1.57075 0 0" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.05" radius="0.035"/>
        </geometry>
        <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.035"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
    </link>
    <joint name="front_wheel_joint" type="continuous">
      <axis rpy="0 0 0" xyz="0 1 0"/>
      <parent link="base_link"/>
      <child link="front_wheel"/>
      <origin rpy="0 0 0" xyz="0.1 0 -0.085"/>
    </joint>

    <gazebo reference="front_wheel">
      <mu1 value="200.0"/>
      <mu2 value="100.0"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <material>Gazebo/Grey</material>
    </gazebo>

  <link name="center_thing">
    <visual>
      <geometry>
        <box size="0.05 0.2 0.3"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.2 0.3"/>
      </geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.3"/>
    </collision>
    <inertial>
      <mass value="5"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

    <gazebo reference="center_thing">
      <mu1 value="200.0"/>
      <mu2 value="100.0"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <material>Gazebo/Grey</material>
    </gazebo>

  <joint name="base_to_center_thing" type="fixed">
    <parent link="base_link"/>
    <child link="center_thing"/>
    <origin xyz="0.12 0 0.2"/>
  </joint>



  <link name="right_thing">
    <visual>
      <geometry>
        <box size="0.05 0.3 0.3"/>
      </geometry>
      <origin rpy="0 0 -0.7" xyz="0 0.2 0"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.3 0.3"/>
      </geometry>
      <origin rpy="0 0 -0.7" xyz="0 0.2 0"/>
    </collision>
    <inertial>
      <mass value="5"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <joint name="base_to_right_thing" type="fixed">
    <parent link="base_link"/>
    <child link="right_thing"/>
    <origin xyz="0.2 0 0.2"/>
  </joint>

    <gazebo reference="right_thing">
      <mu1 value="200.0"/>
      <mu2 value="100.0"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <material>Gazebo/Grey</material>
    </gazebo>

  <link name="left_thing">
    <visual>
      <geometry>
        <box size="0.05 0.3 0.3"/>
      </geometry>
      <origin rpy="0 0 0.7" xyz="0 -0.2 0"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.3 0.3"/>
      </geometry>
      <origin rpy="0 0 0.7" xyz="0 -0.2 0"/>
    </collision>
    <inertial>
      <mass value="5"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

    <gazebo reference="left_thing">
      <mu1 value="200.0"/>
      <mu2 value="100.0"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <material>Gazebo/Grey</material>
    </gazebo>


  <joint name="base_to_left_thing" type="fixed">
    <parent link="base_link"/>
    <child link="left_thing"/>
    <origin xyz="0.2 0 0.2"/>
  </joint>

  <gazebo>
	<plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
		<robotNamespace>/</robotNamespace>
	</plugin>
  </gazebo>
</robot>
